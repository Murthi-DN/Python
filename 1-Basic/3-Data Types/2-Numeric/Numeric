
--------------------------------------------------------------------------------------------

#Int
a=10
print(a)
print(type(a))

--------------------------------------------------------------------------------------------

#Float
b=1.5
print(b)
print(type(b))

--------------------------------------------------------------------------------------------

#Boolean
g=True
print(type(g))

--------------------------------------------------------------------------------------------

#Type Conversion
h=int(b)

i=float(a)
print(i)

#String to Float
j=float("1.5")
