
n=int(input("Enter number:"))
temp=n
rev=0

while(n>0):
	rem=n%10
	rev=rev*10+rem
	n=n//10

if(temp==rev):
	print("Palindrome")

else:
	print("Not Palindrome")






or 




string = str(num)
string = list(string)
string.reverse()
string = ''.join(string) #list to str
num = int(string)

print(num)

